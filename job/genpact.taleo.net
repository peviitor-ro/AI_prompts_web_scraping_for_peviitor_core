           { "id": "LOCATION", "selectedValues": [] },
           { "id": "JOB_FIELD", "selectedValues": [] },
           { "id": "JOB_NUMBER", "selectedValues": [] },
           { "id": "URGENT_JOB", "selectedValues": [] },
           { "id": "EMPLOYEE_STATUS", "selectedValues": [] },
           { "id": "STUDY_LEVEL", "selectedValues": [] },
